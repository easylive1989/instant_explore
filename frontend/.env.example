# Google APIs Configuration
# Get your API keys from https://console.cloud.google.com/

# Google Places API (New) - 用於地點搜尋和詳細資訊
GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# Google Maps SDK - 用於地圖顯示
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Google Directions API - 用於路線規劃
GOOGLE_DIRECTIONS_API_KEY=your_google_directions_api_key_here

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Google OAuth Configuration
# Get these from Google Cloud Console OAuth 2.0 credentials
GOOGLE_WEB_CLIENT_ID=your_google_web_client_id_here
GOOGLE_IOS_CLIENT_ID=your_google_ios_client_id_here

# Environment
ENVIRONMENT=development